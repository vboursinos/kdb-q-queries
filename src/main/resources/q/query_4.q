joinTable: aj[`id;
    select from
        update popUpdated: (pop+100) * 1 from
        update pop: (pop + 100) * 1 from
            select from
                update pop: (pop + 100) * 1 from
                    select pop: pop * 1 from
                        select from
                            update pop: (pop+100) * 1 from
                                select from city
                            where pop > 4000000 - 200;
`id xkey country]